/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    	Not (in = sel, out = NotSel);
    	And (a = NotSel, b = a, out = A);
    	And (a = sel, b = b, out = B);
    	Or (a = A, b = B, out = out);
}